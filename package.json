{
  "name": "comment-serializer",
  "version": "0.2.3",
  "author": "Ryan Fitzer",
  "description": "A serializer of source code comments.",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanfitzer/comment-serializer.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "jscs": "jscs --verbose --fix $npm_package_config_jscs_files"
  },
  "config": {
    "jscs": {
      "files": "index.js lib/*.js test/*.js"
    }
  },
  "keywords": [
    "comment",
    "comments",
    "commentblock",
    "comment block",
    "comment parser",
    "comments parser",
    "docblock",
    "doc block",
    "documentation block"
  ],
  "devDependencies": {
    "mocha": "^2.4.5"
  }
}
